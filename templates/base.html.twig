<!DOCTYPE html>
<html class="no-js" lang="en" dir="ltr">
<head>

    <title>Food Shack Company</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <meta name="viewport" content="user-scalable=no, width=device-width, height=device-height, initial-scale=1, maximum-scale=1, minimum-scale=1, minimal-ui" />

    <meta name="Food Shack Company" content="Food Shack Company" />
    <meta name="Food Shack Company" content="Food Shack Company, Kuwait" />
    <meta name="Food Shack Company" content="Food Shack Company, Sauces, Kuwait" />
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="{{ '/media/images/icons/favicon.png' }}"/>

    {{ encore_entry_link_tags('app') }}

    {% block stylesheets %}{% endblock %}
    {% block javascript %}
    {% endblock %}
    <!--===============================================================================================-->
</head>
<body {% if page is defined and page != 'home' %} class="woocommerce-page product-page" {% endif %}>
<div id="app">
    <!-- start header -->
    <header id="top-bar" class="top-bar {% if page is defined and page == 'home' %} top-bar--style-1 {% else %} top-bar--style-2 {% endif %} ">
        <div class="top-bar__bg"></div>

        <div class="container">
            <div class="row align-items-center justify-content-between no-gutters">

                <a class="{% if page is defined and page == 'home' %} top-bar__logo1 {% else %} top-bar__logo {% endif %}site-logo" href="{{ path('home', {locate: 'en'}) }}">
                    <img class="img-fluid" src="{{ '/media/images/site_logo.png' }}" alt="Food Shack Company" title="Food Shack Company" />
                </a>

                <a id="top-bar__navigation-toggler" class="top-bar__navigation-toggler top-bar__navigation-toggler--light" href="javascript:void(0);"><span></span></a>

                <div id="top-bar__inner" class="top-bar__inner">
                    <div>
                        <nav id="top-bar__navigation" class="top-bar__navigation navigation" role="navigation">
                            <ul>
                                <li {% if page is defined and page == 'home' %} class="active" {% endif %}>
                                    <a href="{{ path('home', {locate: 'en'}) }}">Home</a>
                                </li>

                                <li {% if page is defined and page == 'about.us' %} class="active" {% endif %}>
                                    <a href="{{ path('about.us', {locate: 'en'}) }}">About</a>
                                </li>

                                <li>
                                    <a href="hot-stuff.html">The hot stuff</a>
                                </li>

                                <li class="has-submenu">
                                    <a href="javascript:void(0);">Web store</a>

                                    <ul class="submenu">
                                        {% for category in categories %}
                                            <li><a href="{{ path('category', {slug: category.slug}) }}">{{ category.name }}</a></li>
                                        {% endfor %}
                                    </ul>
                                </li>




                                <li {% if page is defined and page == 'contact.us' %} class="active" {% endif %}>
                                    <a href="{{ path('contact.us', {locate: 'en'}) }}">Contact</a>
                                </li>

                                <li class="li-profile">
                                    <a href="{{ path('login', { locale: 'en'}) }}"><i class="fontello-profile"></i></a>
                                </li>
                                <li class="li-cart">
                                    <a href="cart-food.html"><i class="fontello-shopping-bag"></i><span class="total">3</span></a>
                                </li>

                            </ul>
                        </nav>
                    </div>
                </div>

            </div>
        </div>
    </header>
    <!-- end header -->

    {% block body %}{% endblock %}

    <!-- start footer -->
    <footer id="footer" class="footer--style-1">
        <div class="container">
            <div class="row">
                <div class="col-12 col-lg-4 text-center-mobile">
                    <div class="footer__item">
                        <a class="site-logo" href="index.html">
                            <img class="img-fluid  lazy" src="{{ '/media/images/blank.gif' }}" data-src="{{ '/media/images/site_logo.png' }}" alt="demo" />
                        </a>
                    </div>


                </div>

                <div class="col-12 col-lg-4 text-center-mobile">
                    <div class="footer__item">
                        <h5 class="h6">Quick links</h5>
                        <hr class="hr-footer" />
                        <ul>
                            <li><a href="#">SAUCES (MIC'S CHILLI)</a></li>
                            <li><a href="#">SANDWICHES (DRESSEDDELI)</a></li>
                            <li><a href="#">CROUTONS</a></li>
                            <li><a href="#">VEGETABLES</a></li>
                        </ul>

                    </div>
                </div>

                <div class="col-12 col-lg-4 text-center-mobile">
                    <div class="footer__item">
                        <h5 class="h6">Connect with us</h5>
                        <hr class="hr-footer" />
                        <div class="social-btns">
                            <a href="#"><i class="fontello-youtube"></i></a>
                            <a href="#"><i class="fontello-twitter"></i></a>
                            <a href="#"><i class="fontello-facebook"></i></a>
                            <a href="#"><i class="fontello-linkedin-squared"></i></a>
                        </div>

                    </div>
                </div>
            </div>


            <div class="row">


                <div class="col-12 col-lg-12 text-center reserverd-footer">
                    <div class="footer__item">
                        <span class="__copy">Â© 2021 FOOD SHACK COMPANY. All rights reserved.</span>
                    </div>
                </div>
            </div>

        </div>
    </footer>
    <!-- end footer -->
</div>

<div id="btn-to-top-wrap">
    <a id="btn-to-top" class="circled" href="javascript:void(0);" data-visible-offset="800"></a>
</div>

<!--===============================================================================================-->
{{ encore_entry_script_tags('app') }}

{% block javascripts %}{% endblock %}
<!--===============================================================================================-->

</body>
</html>

