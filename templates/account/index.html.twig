{% extends 'base.html.twig' %}

{% block body %}
    <!-- start hero -->
    <div id="hero" class="jarallax" data-speed="0.7" data-img-position="50% 100%" style="background-image: url({{ '/media/images/intro_img/11.jpg' }});color: #333;">
        <div class="container">
            <div class="row">
                <div class="col-12 col-md-12">
                    <h2 class="__title"><span>Food Shack Company</span>My Profile</h2>


                </div>
            </div>
        </div>
    </div>
    <!-- end hero -->
    <!-- start main -->
    <main role="main">
        <!-- Common styles
        ================================================== -->

        <section class="container-top container">

            <div class="row m-b-30">
                {% include 'account/side_bar.html.twig' %}
                <div class="col-xs-12 col-sm-12 col-md-8 col-lg-9">
                    <div class="edit_prof_bg1 py-2">
                        <div class="row">
                            <div class="col-md-12"><h4> My profile</h4></div>
                        </div>

                    </div>

                    <div class="box">


                        <div class="row table-pad table-min">


                            <table class="table table-sm table-bordered ">
                                <tbody>
                                <tr>
                                    <td><strong> Name </strong></td>
                                    <td>{{ customer.fullName }}</td>
                                </tr>
                                <tr>
                                    <td><strong> Email </strong></td>
                                    <td> {{ customer.email }}</td>
                                </tr>
                                <tr>
                                    <td><strong> Mobile </strong></td>
                                    <td> {{ customer.phone }}</td>
                                </tr>

                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            </div>


        </section>

    </main>
    <!-- end main -->

{% endblock %}

