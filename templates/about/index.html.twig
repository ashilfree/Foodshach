                </div>

                <!-- start timeline -->
                <div class="timeline">
                    <div class="__inner">
                        <div class="row">
                            <!-- start item -->
                            <div class="col-12 col-md-3">
                                <div class="__item">
                                    <i class="__ico"></i>

