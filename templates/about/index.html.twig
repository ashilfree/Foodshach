                        </p>

                        <p>
                            {{ about.description3 }}
                        </p>

                        <p>
                            {{ about.description4 }}
                        </p>

                        <p>
                            <a class="custom-btn custom-btn--medium custom-btn--style-1" href="{{ about.btnUrl }}">Get in Touch</a>
                        </p>
                    </div>
                </div>
            </div>
        </section>
        <!-- end section -->


        <!-- start section -->
        <section class="section section--gutter section--base-bg section--custom-02 mt-60">
